    {
        "id": "70f1aace271b00c8",
        "type": "ui-template",
        "z": "f69312076bb95996",
        "group": "16a799820a185e70",
        "page": "",
        "ui": "",
        "name": "Finish Button",
        "order": 9,
        "width": "4",
        "height": 2,
        "head": "",
        "format": "<template>\n  <div class=\"text-center pa-4\">\n    <v-btn\n      class=\"finish-style\"\n      height=\"100%\"\n      width=\"100%\"\n      @click=\"onFinishClick\"\n    >\n      Finish\n    </v-btn>\n\n    <v-dialog v-model=\"dialog\" width=\"700px\">\n      <v-card title=\"Confirm Finish\">\n        <v-card-text>\n          <v-number-input\n            v-model=\"rejectQty\"\n            :reverse=\"false\"\n            controlVariant=\"split\"\n            label=\"Output Quantity\"\n            :hideInput=\"false\"\n            :inset=\"false\"\n            variant=\"outlined\"\n          />\n        </v-card-text>\n\n        <v-card-actions class=\"actions-right\">\n          <v-btn text @click=\"dialog = false\">\n            Cancel\n          </v-btn>\n\n          <v-btn\n            class=\"submit-big\"\n            color=\"red\"\n            variant=\"flat\"\n            @click=\"submit\"\n          >\n            Submit\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      dialog: false,\n      rejectQty: null,\n      currentCount: 0,\n      machineId: \"\",\n    };\n  },\n\n  watch: {\n    msg(newMsg) {\n      if (!newMsg) return;\n\n      if (newMsg.type === \"machineId\") {\n        this.machineId = newMsg.value || \"\";\n      }\n\n      if (newMsg.type === \"currentCount\") {\n        this.currentCount = Number(newMsg.value) || 0;\n      }\n\n      if (this.isAutoFinishMachine()) {\n        this.rejectQty = this.currentCount;\n      }\n    },\n  },\n\n  methods: {\n    isAutoFinishMachine() {\n      const id = this.machineId?.trim(); \n      return (\n        id === \"TMM001\" ||\n        id === \"BPM001\" ||\n        id === \"ZTP001\" \n        || id === \"TEST001\"\n      );\n    },\n\n    onFinishClick() {\n      \n      if (!this.machineId) return;\n\n      if (this.isAutoFinishMachine()) {\n        this.rejectQty = this.currentCount;\n        this.sendPayload();\n        return; \n      }\n      \n      // Only open dialog for non-auto-finish machines\n      this.dialog = true;\n    },\n\n    submit() {\n      this.sendPayload();\n      this.dialog = false;\n    },\n\n    sendPayload() {\n      const reject = Number(this.rejectQty ?? 0);\n      const payloadValue = `${this.currentCount - reject}`;\n      this.send({\n        payload: {\n          rejectQty: payloadValue,\n        },\n      });\n      this.$root.$router.push(\"/production_plan\");\n    },\n  },\n};\n</script>\n\n\n\n<style scoped>\n.finish-style {\n  font-weight: 600;\n  color: white !important;\n  background: #e73c52 !important;\n  border: 0.1vw solid rgba(255, 255, 255, 0.2);\n  border-radius: 14px;\n  box-shadow:\n    0 4px 10px rgba(1, 39, 76, 0.4),\n    inset 0 0 12px rgba(255, 255, 255, 0.15);\n  text-transform: none;\n  letter-spacing: 0.5px;\n  font-size: calc(0.8vw + 0.4em);\n  padding: 0.6em 1.2em;\n  transition: all 0.25s ease;\n  cursor: pointer;\n}\n\n.v-number-input .v-icon {\n  color: #444444 !important;\n}\n\n.finish-style:hover,\n.finish-style:focus {\n  background: #b52f43 !important;\n  box-shadow:\n    0 6px 16px rgba(1, 39, 76, 0.5),\n    inset 0 0 14px rgba(255, 255, 255, 0.2);\n  border-color: rgba(255, 255, 255, 0.4);\n}\n\n.finish-style:active {\n  box-shadow: inset 0 0 10px rgba(0, 0, 0, 0.4);\n}\n\n.submit-big {\n  min-width: 180px;\n  padding: 20px 24px;\n  font-size: 1rem;\n  font-weight: 600;\n  border-radius: 10px;\n}\n\n.actions-right {\n  display: flex;\n  justify-content: flex-end;\n  gap: 12px;\n  padding-right: 20px;\n}\n</style>\n",
